---
interface Props {
	title: string;
	body: string;
	href: string;
}
const { href, title, body } = Astro.props;
import ExternalLinks from '../components/ExternalLinks.astro'
---
<header class="p-12 grid place-content-center">

	<a href="/">Home</a>
	<a href="/bio">Bio</a>

	<img src = "/img/cabrochico.svg" alt="Logo CABROCHICO" class=""/>
	<p class="text-center text-white font-semibold ">
		Somos cabrochico y queremos jugar!
	</p>
	<ExternalLinks/>
</header>

<style>
	img {
		width: 450px;
		height: auto;
	}
</style>
